
@{
    ViewBag.Title = "Do you meet the requirements?";
}

<div class="content">

    <h1>@ViewBag.Title</h1>

    <h2 style="font-size: 200%;">
        One or more of your answers disqualify you from the
        <br />
        position(s) you are applying for.
        <br />
        <br />
        Please contact us at 1-800-968-7825 for more information,
        <br />
        or click below to continue searching for positions.
    </h2>

    <br />

    <input type="submit" value="Continue" onclick="location.href='@Url.Action("Index", "LocalJobs")'" class="label-green text-shadow" />

</div>